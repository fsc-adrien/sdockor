  ðŸ§° Menu list section
{%- endcomment -%}

{%- liquid
  assign section_heading = section.settings.section_heading | strip
  assign text_color = section.settings.text_color
-%}

<section
  id="{{ section.id }}"
  class="
    pxu-section
    pxu-menu-list
    pxu-section--animated-{{ section.settings.enable_animations }}
  "
  style="
    --pxu-section-heading-size: {{ section.settings.section_heading_size }}px;
    --pxu-section-heading-size-mobile: {{ section.settings.section_heading_size_mobile }}px;
    --pxu-section-block-heading-size: {{ section.settings.block_heading_size }}px;
    --pxu-section-block-heading-size-mobile: {{ section.settings.block_heading_size_mobile }}px;
    --pxu-section-block-text-size: {{ section.settings.block_text_size }}px;
    --pxu-section-block-text-size-mobile: {{ section.settings.block_text_size_mobile }}px;
    --pxu-section-text-color: {{ text_color }};
    --pxu-section-link-color: {{ section.settings.link_color }};
    --pxu-btn-text-color: {{ text_color }};
    --pxu-section-text-color-alpha-15: {{ text_color | color_modify: 'alpha', 0.15 }};
    --pxu-section-text-color-alpha-65: {{ text_color | color_modify: 'alpha', 0.65 }};
    --pxu-section-background-color: {{ section.settings.background_color }};
    --pxu-section-placeholder-fill-color: {{ text_color | color_modify: 'alpha', 0.35 }};
    --pxu-section-placeholder-background-color: {{ text_color | color_modify: 'alpha', 0.15 }};
    --pxu-section-padding-top: {{ section.settings.padding_top }}px;
    --pxu-section-padding-bottom: {{ section.settings.padding_bottom }}px;
    --pxu-section-margin-top: {{ section.settings.margin_top }}px;
    --pxu-section-margin-bottom: {{ section.settings.margin_bottom }}px;
    --pxu-section-content-alignment: {{ section.settings.content_alignment }};
    --pxu-section-desktop-columns: {{ section.settings.desktop_columns }};
    --pxu-section-mobile-columns: {{ section.settings.mobile_columns }};
  "
  data-pxu-menu-list
>
  <script
    type="application/json"
    data-component-data
  >
    {
      "id": "{{ section.id }}",
      "handle": "pxu-menu-list",
      "className": "MenuListSection",
      "name": "Menu list",
      "version": "1.0.2",
      "scriptsUrl": "{{ 'scripts.js' | asset_url | replace: '/assets/', '/compiled_assets/' }}"
    }
  </script>

  <div class="pxu-section__container pxu-menu-list__container">
    <div class="heading-container">
      {%- if section_heading != blank -%}
        <h2 class="pxu-section__section-heading pxu-menu-list__section-heading">
          {{- section_heading -}}
        </h2>
      {%- endif -%}
    </div>
    <div class="pxu-menu-list__menus pxu-menu-list--blocks-count-{{ section.blocks.size }}">
      {% for block in section.blocks %}
        {% case block.type %}
          {%- when 'menu' -%}
            {%- liquid
              assign menu = block.settings.menu
              assign image = block.settings.image | default: menu.image
              assign heading = block.settings.heading | default: block.settings.menu.title | default: 'Menu' | escape

              if block.settings.image
                assign image = block.settings.image
              elsif menu
                for link in linklists[menu].links
                  if link.object.image
                    assign image = link.object.image
                    break
                  elsif link.object.featured_image
                    assign image = link.object.featured_image
                  elsif collections[link.object.handle].products.first.featured_media.preview_image
                    assign image = collections[link.object.handle].products.first.featured_media.preview_image
                    break
                  endif
                endfor
              endif
            -%}

            <article
              class="pxu-menu-list__menu pxu-menu-list__menu-{{ forloop.index }}"
              {{ block.shopify_attributes }}
              data-pxu-menu-list-block
            >
              <div class="pxu-menu-list__menu-wrapper" data-pxu-menu-list-menu-wrapper>
                <figure
                  class="pxu-menu-list__menu-image-wrapper"
                  data-pxu-menu-list-toggle-mobile
                  {% if image %}
                    style="background-image: url('{{- image | image_url: width: 1024, height: 2024 -}}');"
                  {%- endif -%}
                >
                  {%- unless image -%}
                    {{- 'collection-1' | placeholder_svg_tag: 'pxu-placeholder' -}}
                  {%- endunless -%}
                </figure>
                <div class="pxu-menu-list__menu-content-wrapper">
                  <h3
                    class="
                      pxu-menu-list__menu-heading
                      pxu-heading
                    "
                    data-pxu-menu-list-toggle-mobile
                  >
                    {{- heading -}}
                    <svg
                      class="pxu-menu-list__icon"
                      focusable="false"
                      role="presentation"
                      xmlns="http://www.w3.org/2000/svg"
                      width="14"
                      height="8"
                      viewBox="0 0 14 8"
                      fill="none"
                      data-closed
                    >
                      <path class="pxu-menu-list__icon--left" d="M7 6.75L12.5 1.25" stroke="currentColor" stroke-width="1.75" stroke-linecap="square" data-pxu-chevron-left></path>
                      <path class="pxu-menu-list__icon--right" d="M7 6.75L1.5 1.25" stroke="currentColor" stroke-width="1.75" stroke-linecap="square" data-pxu-chevron-right></path>
                    </svg>
                  </h3>
                  {% assign show_max_items = 6 %}
                  <ul
                    class="pxu-menu-list__list"
                    data-pxu-menu-list-content
                    data-hidden
                  >
                    {% comment %} Blank links as onboarding content {% endcomment %}
                    {%- if menu == null -%}
                      {% for n in (1..6) %}
                        <li
                          class="pxu-menu-list__list-item"
                        >
                          <a href="#">Link</a>
                        </li>
                      {% endfor %}
                    {%- endif -%}
                    {% for link in menu.links %}
                      {% if forloop.index > show_max_items %}
                        <li class="pxu-menu-list__list-item pxu-menu-list__list-item--overflow">
                          <ul
                            class="
                              pxu-menu-list__list
                              pxu-menu-list__list--overflow
                            "
                            data-pxu-menu-list-overflow
                            data-hidden
                          >
                            {% for hidden_link in linklists[menu].links %}
                              {% if forloop.index <= show_max_items %}
                                {% continue %}
                              {% endif %}
                              <li
                                class="pxu-menu-list__list-item"
                              >
                                <a href="{{ hidden_link.url }}">
                                  {{ hidden_link.title }}
                                </a>
                              </li>
                            {% endfor %}
                          </ul>
                          <button
                            class="pxu-menu-list__overflow-toggle"
                            data-pxu-menu-list-toggle
                          >
                            <span class="pxu-menu-list__show-more">Show more</span>
                            <span class="pxu-menu-list__show-less">Show less</span>
                          </button>
                        </li>
                        {% break %}
                      {% endif %}
                      <li
                        class="pxu-menu-list__list-item"
                      >
                        <a href="{{ link.url }}">
                          {{ link.title }}
                        </a>
                      </li>
                    {% endfor %}
                  </ul>
                </div>
              </div>
            </article>
        {%- endcase -%}
      {%- endfor -%}
    </div>
  </div>
</section>

