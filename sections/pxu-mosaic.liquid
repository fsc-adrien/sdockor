{%- comment -%}
  ðŸ§° Mosaic section
{%- endcomment -%}

{%- liquid
  assign background_color = section.settings.background_color

  assign remove_space = section.settings.remove_space

  assign num_blocks = section.blocks | size
  assign has_slider = false
  if section.settings.mobile_layout == "slider" and num_blocks > 1
    assign has_slider = true

    assign background_brightness = background_color | color_brightness
    assign dots_color = "#000"
    if background_brightness < 127.5
      assign dots_color = "#FFF"
    endif
  endif

  assign loading = 'lazy'
  assign fetch_priority = 'auto'
  if section.index <= 3
    assign loading = 'eager'
    assign fetch_priority = 'high'
  endif
-%}

<section
  id="{{ section.id }}"
  class="
    pxu-section
    pxu-section--animated-{{ section.settings.enable_animations }}
    pxu-mosaic
    pxu-mosaic--mobile-layout-{{ section.settings.mobile_layout }}
    pxu-mosaic--align-{{ section.settings.content_alignment }}
    pxu-mosaic--item-count-{{ num_blocks }}
    pxu-mosaic--desktop-height-{{ section.settings.desktop_section_height }}
    {% if remove_space %}pxu-mosaic--remove-space{% endif %}
  "
  style="
    --pxu-section-background-color: {{ background_color }};
    --pxu-section-padding-top: {{ section.settings.padding_top }}px;
    --pxu-section-padding-bottom: {{ section.settings.padding_bottom }}px;
    --pxu-section-margin-top: {{ section.settings.margin_top }}px;
    --pxu-section-margin-bottom: {{ section.settings.margin_bottom }}px;
    --pxu-section-corner-radius: {{ section.settings.corner_radius }}px;
    --pxu-section-mobile-image-height: {{ section.settings.mobile_image_height }}px;
  "
  data-pxu-mosaic
>
  <script
    type="application/json"
    data-component-data
  >
    {
      "id": "{{ section.id }}",
      "handle": "pxu-mosaic",
      "className": "MosaicSection",
      "name": "Mosaic",
      "version": "1.0.0",
      "scriptsUrl": "{{ 'scripts.js' | asset_url | replace: '/assets/', '/compiled_assets/' }}",
      "hasSlider": {{ has_slider }}
    }
  </script>

  <div class="pxu-section__container pxu-mosaic__container">
    <div class="pxu-mosaic__inner">
      {% if has_slider %}
        <div
          class="
            pxu-carousel
            pxu-carousel--spaced
            pxu-carousel--sm-down
          "
        >
          <div
            class="pxu-carousel__items"
            data-pxu-carousel="xxs,sm"
          >
            {% for block in section.blocks %}
              {% liquid
                assign image = block.settings.image
                assign mobile_image = block.settings.mobile_image

                assign placeholder_number = forloop.index0 | modulo: 2 | plus: 1
                assign placeholder = 'lifestyle-' | append: placeholder_number

                assign heading = block.settings.heading | strip
                assign text = block.settings.text | strip

                assign button_label = block.settings.button_label | escape
                assign button_url = block.settings.button_url
                assign button_style = block.settings.button_style
                assign button_text_color = block.settings.button_text_color
                assign button_background_color = block.settings.button_background_color
                assign button_brightness = button_background_color | color_brightness
                assign button_brightness_mod = 'light-on-dark'
                if button_brightness > 127.5
                  assign button_brightness_mod = 'dark-on-light'
                endif

                assign image_overlay_opacity = block.settings.image_overlay_opacity | divided_by: 100.0

                assign desktop_image_size = "33.33vw"
                case forloop.index
                  when 1
                    if num_blocks == 1
                      assign desktop_image_size = "100vw"
                    else
                      assign desktop_image_size = "66.66vw"
                    endif
                  when 2
                    if num_blocks == 5
                      assign desktop_image_size = "66.66vw"
                    endif
                  when 4
                    if num_blocks == 4
                      assign desktop_image_size = "66.66vw"
                    endif
                endcase

                assign all_sizes_attr = "(max-width: 719px) 100vw, (min-width: 720px) " | append: desktop_image_size
              %}
              <div
                class="pxu-carousel__item"
                data-pxu-carousel-item
                data-pxu-carousel-item-color="{{ dots_color }}"
              >
                <div
                  id="{{ block.id }}"
                  class="
                    pxu-mosaic__item
                    pxu-mosaic__item--pos-{{ forloop.index }}
                  "
                  style="
                    --pxu-block-text-color: {{ block.settings.text_color }};
                    --pxu-heading-size: {{ block.settings.heading_size }}px;
                    --pxu-heading-size-mobile: {{ block.settings.heading_size_mobile }}px;
                    --pxu-text-size: {{ block.settings.text_size }}px;
                    --pxu-text-size-mobile: {{ block.settings.text_size_mobile }}px;
                    --pxu-image-overlay-color: {{ block.settings.image_overlay_color }};
                    --pxu-image-overlay-opacity: {{ image_overlay_opacity }};
                    --pxu-focal-point-desktop: {{ image.presentation.focal_point }};
                    --pxu-focal-point-mobile: {{ mobile_image.presentation.focal_point }};
                  "
                  {{ block.shopify_attributes }}
                >
                  {% if image or mobile_image %}
                    <picture class="pxu-mosaic__image">
                      {% if mobile_image %}
                        <source
                          media="(max-width: 719px)"
                          srcset="
                            {{ mobile_image | image_url: width: 840 }} 840w,
                            {{ mobile_image | image_url: width: 720 }} 720w,
                            {{ mobile_image | image_url: width: 420 }} 420w,
                            {{ mobile_image | image_url: width: 360 }} 360w
                          "
                          sizes="100vw"
                        >
                      {% endif %}
                      {% if mobile_image and image == blank %}
                        {{ mobile_image | 
                          image_url: width: 1600 | 
                          image_tag: 
                            loading: loading, 
                            fetchpriority: fetch_priority,
                            widths: "1600, 1100, 768, 420, 360",
                            sizes: all_sizes_attr
                        }}
                      {% else %}
                        {{ image | 
                          image_url: width: 1600 | 
                          image_tag: 
                            loading: loading, 
                            fetchpriority: fetch_priority,
                            widths: "1600, 1100, 768, 420, 360",
                            sizes: all_sizes_attr
                        }}
                      {% endif %}
                    </picture>
                  {% else %}
                    <div class="pxu-mosaic__image">
                      {% if image_overlay_opacity < 1.0 %}
                        {{ placeholder | placeholder_svg_tag: 'pxu-placeholder' }}
                      {% endif %}
                      &#x200B;
                    </div>
                  {% endif %}

                  <div class="pxu-mosaic__content">
                    {%- if heading != blank -%}
                      <h2 class="pxu-mosaic__heading pxu-heading">
                        {{- heading -}}
                      </h2>
                    {%- endif -%}
                    {%- if text != blank -%}
                      <div class="pxu-mosaic__text pxu-rte">
                        {{- text -}}
                      </div>
                    {%- endif -%}
                    {%- if button_label != blank -%}
                      <a
                        class="
                          pxu-mosaic__button
                          pxu-btn
                          pxu-btn--{{ button_style }}
                          pxu-btn--{{ button_brightness_mod }}
                        "
                        style="
                          --pxu-btn-background-color: {{ button_background_color }};
                          --pxu-btn-text-color: {{ button_text_color }};
                        "
                        {% if button_url != blank %}
                          href="{{ button_url }}"
                        {% endif %}
                      >
                        {{- button_label -}}
                      </a>
                    {%- endif -%}
                  </div>
                </div>
              </div>
            {% endfor %}
          </div>
        </div>

      {% elsif has_slider == false %}
        {% for block in section.blocks %}
          {% liquid
            assign image = block.settings.image
            assign mobile_image = block.settings.mobile_image

            assign placeholder_number = forloop.index0 | modulo: 2 | plus: 1
            assign placeholder = 'lifestyle-' | append: placeholder_number

            assign heading = block.settings.heading | strip
            assign text = block.settings.text | strip

            assign button_label = block.settings.button_label | escape
            assign button_url = block.settings.button_url
            assign button_style = block.settings.button_style
            assign button_text_color = block.settings.button_text_color
            assign button_background_color = block.settings.button_background_color
            assign button_brightness = button_background_color | color_brightness
            assign button_brightness_mod = 'light-on-dark'
            if button_brightness > 127.5
              assign button_brightness_mod = 'dark-on-light'
            endif

            assign image_overlay_opacity = block.settings.image_overlay_opacity | divided_by: 100.0

            assign desktop_image_size = "33.33vw"
            case forloop.index
              when 1
                if num_blocks == 1
                  assign desktop_image_size = "100vw"
                else
                  assign desktop_image_size = "66.66vw"
                endif
              when 2
                if num_blocks == 5
                  assign desktop_image_size = "66.66vw"
                endif
              when 4
                if num_blocks == 4
                  assign desktop_image_size = "66.66vw"
                endif
            endcase

            assign all_sizes_attr = "(max-width: 719px) 100vw, (min-width: 720px) " | append: desktop_image_size
          %}

          <div
            id="{{ block.id }}"
            class="
              pxu-mosaic__item
              pxu-mosaic__item--pos-{{ forloop.index }}
            "
            style="
              --pxu-block-text-color: {{ block.settings.text_color }};
              --pxu-heading-size: {{ block.settings.heading_size }}px;
              --pxu-heading-size-mobile: {{ block.settings.heading_size_mobile }}px;
              --pxu-text-size: {{ block.settings.text_size }}px;
              --pxu-text-size-mobile: {{ block.settings.text_size_mobile }}px;
              --pxu-image-overlay-color: {{ block.settings.image_overlay_color }};
              --pxu-image-overlay-opacity: {{ image_overlay_opacity }};
              --pxu-focal-point-desktop: {{ image.presentation.focal_point }};
              --pxu-focal-point-mobile: {{ mobile_image.presentation.focal_point }};
            "
            {{ block.shopify_attributes }}
          >
            {% if image or mobile_image %}
              <picture class="pxu-mosaic__image">
                {% if mobile_image %}
                  <source
                    media="(max-width: 719px)"
                    srcset="
                      {{ mobile_image | image_url: width: 840 }} 840w,
                      {{ mobile_image | image_url: width: 720 }} 720w,
                      {{ mobile_image | image_url: width: 420 }} 420w,
                      {{ mobile_image | image_url: width: 360 }} 360w
                    "
                    sizes="100vw"
                  >
                {% endif %}
                {% if mobile_image and image == blank %}
                  {{ mobile_image | 
                    image_url: width: 1600 | 
                    image_tag: 
                      loading: loading, 
                      fetchpriority: fetch_priority,
                      widths: "1600, 1100, 768, 420, 360",
                      sizes: all_sizes_attr
                  }}
                {% else %}
                  {{ image | 
                    image_url: width: 1600 | 
                    image_tag: 
                      loading: loading, 
                      fetchpriority: fetch_priority,
                      widths: "1600, 1100, 768, 420, 360",
                      sizes: all_sizes_attr
                  }}
                {% endif %}
              </picture>
            {% else %}
              <div class="pxu-mosaic__image">
                {% if image_overlay_opacity < 1.0 %}
                  {{ placeholder | placeholder_svg_tag: 'pxu-placeholder' }}
                {% endif %}
                &#x200B;
              </div>
            {% endif %}

            <div class="pxu-mosaic__content">
              {%- if heading != blank -%}
                <h2 class="pxu-mosaic__heading pxu-heading">
                  {{- heading -}}
                </h2>
              {%- endif -%}
              {%- if text != blank -%}
                <div class="pxu-mosaic__text pxu-rte">
                  {{- text -}}
                </div>
              {%- endif -%}
              {%- if button_label != blank -%}
                <a
                  class="
                    pxu-mosaic__button
                    pxu-btn
                    pxu-btn--{{ button_style }}
                    pxu-btn--{{ button_brightness_mod }}
                  "
                  style="
                    --pxu-btn-background-color: {{ button_background_color }};
                    --pxu-btn-text-color: {{ button_text_color }};
                  "
                  {% if button_url != blank %}
                    href="{{ button_url }}"
                  {% endif %}
                >
                  {{- button_label -}}
                </a>
              {%- endif -%}
            </div>
          </div>
        {% endfor %}
      {% endif %}
    </div>
  </div>
</section>

